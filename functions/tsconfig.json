{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"typeRoots": ["node_modules/@types"],
		"target": "es2017"
	},
	"compileOnSave": true,
	"skipLibCheck": true,
	"exclude": ["node_modules", "functions"],
	"include": ["**/*.ts", "**/*.tsx"]
}
