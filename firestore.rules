rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /song/{infoId=**} {
      allow read, write: if true;
    }
    match /hist/{eventId=**} {
      allow read: if true;
    }
    match /feedback/{feedbackId=**} {
      allow write: if true;
    }
    match /book/{bookId=**} {
      allow read, write: if true;
    }
  }
}