rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /song/{infoId=**} {
      allow read, write;
    }
    match /hist/{eventId=**} {
      allow read;
    }
    match /feedback/{feedbackId=**} {
      allow write;
    }
    match /book/{bookId=**} {
      allow read, write;
    }
    match /vote/{voteId=**} {
      allow read, write;
    }
    match /table/{eventId=**} {
      allow read, write;
    }
    match /cvote/{animeId=**} {
      allow read, write;
    }
  }
}