{
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"hosting": {
		"predeploy": "yarn export",
		"public": "out",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [],
		"headers": [
			{
				"source": "sw.js",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "no-cache"
					}
				]
			}
		],
		"cleanUrls": true
	}
}
